{
  "name": "react-gym-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.6",
    "@tanstack/react-query": "^5.51.21",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/compose-function": "^0.0.33",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.93",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.6.8",
    "compose-function": "^3.0.3",
    "concurrently": "^8.2.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.1",
    "json-server": "^1.0.0-alpha.23",
    "ky": "^1.5.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8",
    "styled-reset": "^4.5.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "zustand": "^4.5.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "tsc:w": "tsc -w",
    "start-server": "npx json-server --watch public/db.json --port=3001",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run start-server\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@feature-sliced"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --report-unused-disable-directives --max-warnings 0",
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,md,json,html,css,scss}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "3.3.3",
    "react-app-rewired": "^2.2.1"
  }
}
